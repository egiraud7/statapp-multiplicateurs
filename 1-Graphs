#### Données de PIB (FRA)

## Importation des librairies
if (!require("stats")) {install.packages("stats"); library("stats")} # Télécharger le package Time series

## Importation des données
path <- "/Users/eleagiraud/Documents/ENSAE/Stat'app"
setwd(path)
datafile <- "2023-11-30_Base de données.xlsx"
data <- read.xlsx(datafile)

## Classement des données en fonction des pays
FRA = read.xlsx(datafile, sheet="FR_quarter", na.strings = ".", colNames=TRUE, rowNames=FALSE, detectDates = TRUE)
USA = read.xlsx(datafile, sheet="US_quarter", na.strings = ".", colNames=TRUE, rowNames=FALSE, detectDates = TRUE)
GER = read.xlsx(datafile, sheet="GER_quarter", na.strings = ".", colNames=TRUE, rowNames=FALSE, detectDates = TRUE)

## Recadrage des données de PIB
fra_gdp = FRA[c(1,24)]
usa_gdp = USA[c(1,24)]
ger_gdp = GER[c(1,24)]

## Transformation en séries temporelles (à ajuster)
fra.gdp = ts(fra_gdp, start=c(1990,1), frequency=4)
usa.gdp = ts(usa_gdp, start=c(1990,1), frequency=4)
ger.gdp = ts(ger_gdp, start=c(1990,1), frequency=4)

## Graphiques
ts.plot(fra.gdp, type="l", main="France, Output",col="orange", ylab="GDP")
ts.plot(usa.gdp, type="l", main="USA, Output",col="blue", ylab="GDP")
ts.plot(ger.gdp, type="l", main="Germany, Output",col="green", ylab="GDP")

#tout sur le même
ts.plot(fra.gdp, usa.gdp, ger.gdp,
        gpars=list(xlab="Years", ylab="%", lty=c(1:3), col=c("orange","blue","green")))
legend("topright", legend = c("France output", "USA output", "Germany output"), col = c("orange","blue","green"), lty = 1, bty = "n", cex=0.6, xpd=TRUE)

